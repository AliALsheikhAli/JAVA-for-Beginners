public class Main
{
  public static void main (String args[])
  {
    double i, j, d;
      i = 5;
      j = 10;
    if (i != 0)
      {
	System.out.println ("i does not equal zero");
	d = j / i;
	System.out.print ("j / i is " + d);
      }
    System.out.println ();
  }
}

/*
Predicted Output:
i does not equal to zero
j/i is 2
*/
